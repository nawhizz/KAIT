all	: echo $(RUNUX)/lib/libpisam.a \
	       $(RUNUX)/lib/libpisam.$(MAKESLN)

echo		:
	@echo	HYPUB/LIBSRC/PISAM$(ECHOLANG)

$(RUNUX)/lib/libpisam.a	: $(STATIC_OBJS)
	ar vcr $(RUNUX)/lib/libpisam.a $(STATIC_OBJS)

$(RUNUX)/lib/libpisam.$(MAKESLN)	: $(DYNAMIC_OBJS)
	$(GENSL) -o $(RUNUX)/lib/libpisam.$(MAKESLN) $(MAKESL) $(DYNAMIC_OBJS)


